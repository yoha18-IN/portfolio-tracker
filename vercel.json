  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "env": {
    "NEXT_PUBLIC_INSTANTDB_APP_ID": "@next_public_instantdb_app_id",
    "INSTANT_APP_ADMIN_TOKEN": "@instant_app_admin_token",
